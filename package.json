{
  "name": "lighthouse-design-build",
  "version": "1.0.0",
  "type": "module",
  "description": "Beautiful website for Lighthouse Design Build renovation company",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build:css": "node node_modules/tailwindcss/lib/cli.js -i ./src/css/main.css -o ./dist/css/styles.css --watch",
    "build:css:prod": "node node_modules/tailwindcss/lib/cli.js -i ./src/css/main.css -o ./dist/css/styles.css --minify",
    "build": "node node_modules/tailwindcss/lib/cli.js -i ./src/css/main.css -o ./dist/css/styles.css --minify",
    "dev": "live-server --port=3000 --watch=pages,src",
    "serve": "node server.js",
    "start": "node server.js"
  },
  "keywords": [
    "renovation",
    "design",
    "build",
    "construction"
  ],
  "dependencies": {
    "@aws-sdk/client-s3": "^3.700.0",
    "@aws-sdk/s3-request-presigner": "^3.700.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "alpinejs": "^3.13.3",
    "aos": "^2.3.4",
    "autoprefixer": "^10.4.16",
    "bcryptjs": "^2.4.3",
    "cheerio": "^1.1.2",
    "express": "^5.1.0",
    "express-session": "^1.18.1",
    "memorystore": "^1.6.7",
    "multer": "^2.0.0",
    "node-fetch": "^3.3.2",
    "sharp": "^0.33.5",
    "sql.js": "^1.11.0",
    "swiper": "^11.0.5",
    "tailwindcss": "^3.4.0",
    "uuid": "^11.0.3",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "live-server": "^1.2.2"
  }
}
